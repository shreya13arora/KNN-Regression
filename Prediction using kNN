  {
   "cell_type": "code",
   "execution_count": 171,
   "metadata": {},
   "outputs": [],
   "source": [
    "def euclidean(p,q):\n",
    "    d = 0\n",
    "\n",
    "    for i in range(len(q)):\n",
    "        d = d + (p[i]-q[i])**2\n",
    "       \n",
    "\n",
    "    return(np.sqrt(d))\n",
    "\n",
    "#Use linalg"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "from scipy.spatial import distance\n",
    "def euclidean(p,q):\n",
    "    d = 0\n",
    "\n",
    "    for i in range(len(p)):\n",
    "        dst = distance.euclidean(p[i], q[i])\n",
    "        \n",
    "    return dst"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 196,
   "metadata": {},
   "outputs": [],
   "source": [
    "def KNN(train,Y,test, k):\n",
    "    dist = []\n",
    "    \n",
    "    for i in range(len(train)):\n",
    "        dist.append(euclidean(test, train[i]))\n",
    "        \n",
    "    d = dist.copy()\n",
    "    dist.sort()\n",
    "   \n",
    "    index = []\n",
    "    for i in range(k):\n",
    "        indx = d.index(dist[i])\n",
    "        index.append(indx)\n",
    "        d[indx] = -1\n",
    "        \n",
    "    pred_y = np.mean(Y.iloc[index])\n",
    "    return(pred_y)\n",
    "    "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 206,
